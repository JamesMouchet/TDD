use Illuminate\Foundation\Testing\RefreshDatabase;
use Exception;

/**
 * Class DonationFeeTest
 * @package Tests\Unit
 */
class ProjectTest extends TestCase {
    public function testBasicTest()
    {
        $response = $this->get('/projet');
        $response->assertStatus(200);
    }

